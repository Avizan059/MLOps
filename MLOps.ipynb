    "def Drift_stats(df_train,df_prod):\n",
    "    \n",
    "    from scipy.stats import ttest_ind\n",
    "    df_train.pop('y')\n",
    "    Per_Zeros = []\n",
    "    Per_missing_v = []\n",
    "    min_ratio = []\n",
    "    Max_ratio = []\n",
    "    median_ratio = []\n",
    "    mean_ratio = []\n",
    "    std_ratio = []\n",
    "    x=[]\n",
    "    y=[]\n",
    "    z=[]\n",
    "    x_p  = []\n",
    "    y_p = []\n",
    "    z_p = []\n",
    "    u = []\n",
    "    u_p = []\n",
    "    v = []\n",
    "    v_p = []\n",
    "    w = []\n",
    "    w_p = []\n",
    "    Z = 0\n",
    "    M = 0\n",
    "    mean = 0\n",
    "    std = 0\n",
    "    median = 0\n",
    "    mini = 0\n",
    "    maxi = 0\n",
    "    TTest = 0\n",
    "    ttest = []\n",
    "    df_t = df_train.describe()\n",
    "    df_p = df_prod.describe()\n",
    "    # gettig column values from df_train\n",
    "    for i,j in zip(df_train.columns,[x,y,z]):\n",
    "        \n",
    "        j.extend(df_train[i].values.tolist())\n",
    "        \n",
    "    # Getting Column values from df_prod\n",
    "    for i,j in zip(df_prod.columns,[x_p,y_p,z_p]):\n",
    "        j.extend(df_prod[i].values.tolist())\n",
    "       \n",
    "    # Getting Column values from df_train.decribe() \n",
    "    for i,j in zip(df_t.columns,[u,v,w]):\n",
    "        j.extend(df_t[i].values.tolist())\n",
    "    # Getting Column values from df_prod.decribe()   \n",
    "    for i,j in zip(df_p.columns,[u_p,v_p,w_p]):\n",
    "        j.extend(df_p[i].values.tolist())\n",
    "\n",
    "    # getting percentage difference of zero values from training and production\n",
    "    for i,j,k in zip([x,y,z],[x_p,y_p,z_p],['z1','z2','z3']):\n",
    "        k = ((i.count(0))/ len(i)) - ((j.count(0))/ len(j))\n",
    "        Per_Zeros.append(k)\n",
    "        Z = Z + k\n",
    "    \n",
    "    # getting percentage difference of missing values from training and production\n",
    "    for i,j in zip([x,y,z],[x_p,y_p,z_p]):\n",
    "        k = (i.count(\"\")/ len(i)) - (j.count(\"\")/ len(j))\n",
    "        Per_missing_v.append(k)\n",
    "        M = M + k \n",
    "    \n",
    "    # getting ratio of prod to training data of mean,std,median, min and max\n",
    "    for i,j in zip(([u,v,w]) ,[u_p,v_p,w_p]):\n",
    "           \n",
    "            \n",
    "            a = j[1]/i[1]\n",
    "            b = j[2]/i[2]\n",
    "            c = j[3]/i[3]\n",
    "            d = j[5]/i[5]\n",
    "            e = j[7]/i[7]\n",
    "            \n",
    "            mean = mean + a\n",
    "            std = std + b\n",
    "            median = median + d\n",
    "            mini = mini + c\n",
    "            maxi = maxi + e\n",
    "            mean_ratio.append(a)\n",
    "            std_ratio.append(b)\n",
    "            min_ratio.append(c)\n",
    "            median_ratio.append(d)\n",
    "            Max_ratio.append(e)\n",
    "    \n",
    "    for i,j in zip([x,y,z],[x_p,y_p,z_p]):\n",
    "        a = ttest_ind(i, j)\n",
    "        ttest.append(a[1])\n",
    "        TTest = TTest + a[1]\n",
    "        \n",
    "    drift_df = pd.DataFrame(list((Per_Zeros,Per_missing_v,mean_ratio,std_ratio,min_ratio,median_ratio,Max_ratio,ttest)), columns =['a', 'b','c'], index = ['Per_Zeros','Per_missing_v','mean_ratio','std_ratio','min_ratio','median_ratio','Max_ratio','ttest'])\n",
    "    \n",
    "    \n",
    "    if TTest < 2.85: # 3*0.95\n",
    "        result = True\n",
    "        return "failed in ttest",drift_df\n",
    "    elif (Z < -10) or (Z > 10): #+- 10%\n",
    "        result = True\n",
    "        return "failed in Z_ratio",drift_df\n",
    "    elif (M < -10) or (M > 10):  #+- 10%\n",
    "        result = True\n",
    "        return "failed in M_ratio",drift_df\n",
    "    elif (mean < 0.70) or (mean > 1.3):  #+- 30%\n",
    "        result = True\n",
    "        return "failed in mean_ratio",drift_df\n",
    "    elif (std < 0.70) or (std > 1.3):  #+- 30%\n",
    "        result = True\n",
    "        return "failed in std_ratio",drift_df\n",
    "    elif (median < 0.70) or (median > 1.3):  #+- 30%\n",
    "        result = True\n",
    "        return "failed in median_ratio".drift_df\n",
    "    elif (mini < 0.70) or (mini > 1.3):  #+- 30%\n",
    "        result = True\n",
    "        return "failed in mini_ratio",drift_df\n",
    "    elif (maxi < 0.70) or (maxi > 1.3):  #+- 30%\n",
    "        result = True\n",
    "        return "failed in max_ratio",drift_df\n",
    "    else :\n",
    "        result = "No data drift"\n",
    "        return result\n",
    "           \n",
    "   "
   ]
